        Rs Rp  Sc : 2 
        P  R  alpha : 2       # definition de alpha
        Pp Rp  alpha : 2
        Q  R beta : 2         # definition de beta
        Qp Rp  beta : 2 
        P  Q gamma : 2        # definition de gamma
        Pp Qp  gamma : 2
