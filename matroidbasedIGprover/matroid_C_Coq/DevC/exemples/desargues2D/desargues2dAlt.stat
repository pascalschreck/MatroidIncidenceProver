        P Qp Rp : 3
        P Qp Op : 3
        P Rp Op : 3             # fin P
        Q R Pp : 3
        Q R Qp : 3
        Q R Rp : 3
        Q R Op : 3
        Q Pp Qp : 3 
        Q Pp Rp : 3 
        Q Pp Op : 3
        Q Qp Rp : 3
        Q Qp Op : 2              # 1 alignement
